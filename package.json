    "@babel/preset-typescript": "^7.24.7",
    "@babel/register": "^7.21.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.3",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "lzox1-rs": "link:src/lzox1-rs/pkg",
    "mocha": "^10.2.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
