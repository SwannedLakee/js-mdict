  "author": "terasum <terasum@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/terasum/js-mdict/issues"
  },
  "homepage": "https://github.com/terasum/js-mdict#readme",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.6",
    "bl": "^2.2.1",
