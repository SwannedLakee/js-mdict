    "build": "babel src --out-dir lib",
    "lint": "eslint src",
    "format": "prettier -w src/*.js"
  },
  "repository": "git+https://github.com/terasum/js-mdict.git",
  "keywords": [
    "mdict",
    "dictionary",
    "mdx",
    "mdd",
